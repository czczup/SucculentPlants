<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data></data>

    <LinearLayout
        android:id="@+id/item_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <com.succulent.wztxy.succulentplants.common.util.MyOneLineView
            android:id="@+id/avatar_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <com.succulent.wztxy.succulentplants.common.util.MyOneLineView
            android:id="@+id/history_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp" />

        <com.succulent.wztxy.succulentplants.common.util.MyOneLineView
            android:id="@+id/contribution_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <com.succulent.wztxy.succulentplants.common.util.MyOneLineView
            android:id="@+id/evaluate_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <com.succulent.wztxy.succulentplants.common.util.MyOneLineView
            android:id="@+id/roast_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <com.succulent.wztxy.succulentplants.common.util.MyOneLineView
            android:id="@+id/share_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <com.succulent.wztxy.succulentplants.common.util.MyOneLineView
            android:id="@+id/about_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <com.succulent.wztxy.succulentplants.common.util.MyOneLineView
            android:id="@+id/logout_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"/>

    </LinearLayout>



</layout>